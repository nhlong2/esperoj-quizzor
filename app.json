{
    "expo": {
        "name": "Esperoj Quizzor",
        "slug": "Esperoj-Quizzor",
        "version": "0.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "bundleIdentifier": "com.esperoj.quizzor",
            "buildNumber": "0.0.0",
            "supportsTablet": true
        },
        "android": {
            "package": "com.esperoj.quizzor",
            "versionCode": 0,
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}